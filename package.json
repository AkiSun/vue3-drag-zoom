{
  "name": "vue3-drag-zoom",
  "private": false,
  "version": "1.0.3",
  "description": "A Drag & Zoom toolkit for Vue 3",
  "author": "AkiSun",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AkiSun/vue3-drag-zoom.git"
  },
  "homepage": "https://github.com/AkiSun/vue3-drag-zoom#readme",
  "files": [
    "dist"
  ],
  "module": "./dist/vue3-drag-zoom.es.js",
  "main": "./dist/vue3-drag-zoom.umd.js",
  "types": "./dist/vue3-drag-zoom.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vue3-drag-zoom.es.js",
      "require": "./dist/vue3-drag-zoom.umd.js"
    }
  },
  "keywords": ["vue3", "typescript", "drag", "zoom"],
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "dts": "vue-tsc --declaration --emitDeclarationOnly",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "^3.2.41"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@vitejs/plugin-vue": "^3.2.0",
    "typescript": "^4.6.4",
    "vite": "^3.2.3",
    "vue-tsc": "^1.0.9"
  }
}
